#include <iostream>
#include <stack>
using namespace std;

int main(){

	stack<int> mystack;

	for (int i=0; i<=10; i++){
		cout << "in: " << i << endl;
		mystack.push(i);
	}

	while(!mystack.empty()){
		cout << "out: " << mystack.top() << endl;
		mystack.pop();
	}
	

	system("pause");
	return 0;
	
}
